    maxZoom: 18,
    id: 'mapbox/streets-v11',
    tileSize: 512,
    zoomOffset: -1,
    accessToken: 'pk.eyJ1IjoidDE1aHMxOSIsImEiOiJja2dyaGx2amQwZjBtMnhxdzBmd2loZ3F5In0.NemiDxQl-m-_CBtwLT4R7Q'
    }).addTo(mymap);
       
        
   
    var marker = L.marker([<%= @location.latitude %>, <%= @location.longitude %>]).addTo(mymap);

          
        
</script>




