<div id="mapid" style="float: right; width: 360px"></div> 
<script> 
      
    var mymap = L.map('mapid').setView([55.010, -3.010], 5);     
          
    L.tileLayer('https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token={accessToken}', {
